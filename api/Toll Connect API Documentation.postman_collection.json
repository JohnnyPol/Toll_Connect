		{
			"name": "Operations",
			"item": [
				{
					"name": "Toll Station Passes",
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "X-OBSERVATORY-AUTH",
								"value": "{{token}}",
								"type": "text"
							}
						],
						"url": {
							"raw": "{{baseURL}}/tollStationPasses/{{tollStationId}}/{{data_from}}/{{date_to}}?format={{format}}",
							"host": [
								"{{baseURL}}"
							],
							"path": [
